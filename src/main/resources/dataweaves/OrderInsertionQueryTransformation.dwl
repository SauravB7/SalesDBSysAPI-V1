if(vars.orderType == 'online')
	"INSERT INTO " ++ p('db.schema') ++ "." ++ p('db.table.salesOnline') ++ " (ORDER_ID, CUSTOMER_ID, ORDER_STATUS, PURCHASE_TMSTMP, APPROVED_TMSTMP, DELIVERED_CARRIER_DT, DELIVERED_CUSTOMER_DT, ESTIMATED_DELIVERY_DT, ORDER_VALUE, REGION_CD) VALUES (:ORDER_ID, :CUSTOMER_ID, :ORDER_STATUS, TO_TIMESTAMP(:PURCHASE_TMSTMP, 'MM/DD/YYYY HH24:MI:SS'), TO_TIMESTAMP(:APPROVED_TMSTMP, 'MM/DD/YYYY HH24:MI:SS'), TO_DATE(:DELIVERED_CARRIER_DT, 'MM/DD/YYYY'), TO_DATE(:DELIVERED_CUSTOMER_DT, 'MM/DD/YYYY'), TO_DATE(:ESTIMATED_DELIVERY_DT, 'MM/DD/YYYY'), :ORDER_VALUE, :REGION_CD)"
else
	"INSERT INTO " ++ p('db.schema') ++ "." ++ p('db.table.salesOffline') ++ " (ORDER_ID, CUSTOMER_ID, ORDER_STATUS, PURCHASE_TMSTMP, APPROVED_TMSTMP, DELIVERED_CARRIER_DT, DELIVERED_CUSTOMER_DT, ESTIMATED_DELIVERY_DT, ORDER_VALUE, REGION_CD) VALUES (:ORDER_ID, :CUSTOMER_ID, :ORDER_STATUS, TO_TIMESTAMP(:PURCHASE_TMSTMP, 'MM/DD/YYYY HH24:MI:SS'), TO_TIMESTAMP(:APPROVED_TMSTMP, 'MM/DD/YYYY HH24:MI:SS'), TO_DATE(:DELIVERED_CARRIER_DT, 'MM/DD/YYYY'), TO_DATE(:DELIVERED_CUSTOMER_DT, 'MM/DD/YYYY'), TO_DATE(:ESTIMATED_DELIVERY_DT, 'MM/DD/YYYY'), :ORDER_VALUE, :REGION_CD)"